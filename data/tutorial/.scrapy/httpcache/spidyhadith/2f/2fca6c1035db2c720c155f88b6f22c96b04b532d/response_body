    <script type="text/javascript">

        function ViewServiceContent(ServiceHadithMainID,ServiceHadithSayID) {
            document.getElementById('ContentPlaceHolder1_tdHadithFromService').style.display = '';
            var Content;
            $.ajax({ url: "GetServiceContent.aspx?ServiceHadithMainID=" + ServiceHadithMainID + "&ServiceHadithSayID=" + ServiceHadithSayID + ""
                    , async: false, cache: false
            })
                    .success(function (data) {
                        Content = data;
                    })
                    .error(function () { })
                    .complete(function () {
                        var contentArr = new Array();
                        contentArr = Content.split("#@#@");
                        document.getElementById('ContentPlaceHolder1_lblTocPathFromService').innerHTML = contentArr[0];
                        document.getElementById('ContentPlaceHolder1_hidTocPathFromService').value = contentArr[0];

                        document.getElementById('ContentPlaceHolder1_lblContentFromService').innerHTML = contentArr[1];
                        document.getElementById('ContentPlaceHolder1_hidContentFromService').value = contentArr[1];

                        if (document.getElementById('ContentPlaceHolder1_hidServiceIsDiac').value == '') {
                            RemoveDiac('ContentPlaceHolder1_lblContentFromService');
                        }
                    });
        }
    </script>

    <script type="text/javascript">
        function hideHadithFromTakhreegZone() {
            document.getElementById('ContentPlaceHolder1_tdHadithFromService').style.display = 'none';
        
    }

    function viewHadithFromTakhreegZone() {
        document.getElementById('ContentPlaceHolder1_tdHadithFromService').style.display = '';

    }

    function setHidTocPathAndContentFromService() {
        document.getElementById('ContentPlaceHolder1_hidTocPathFromService').value = document.getElementById('ContentPlaceHolder1_lblTocPathFromService').innerHTML;
        document.getElementById('ContentPlaceHolder1_hidContentFromService').value = document.getElementById('ContentPlaceHolder1_lblContentFromService').innerHTML;
    }

    function clearHidTocPathAndContentFromService() {
        document.getElementById('ContentPlaceHolder1_hidTocPathFromService').value = '';
        document.getElementById('ContentPlaceHolder1_hidContentFromService').value = '';
    }

</script>


