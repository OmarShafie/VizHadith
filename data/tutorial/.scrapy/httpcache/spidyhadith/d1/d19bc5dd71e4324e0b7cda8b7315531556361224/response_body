<input type="hidden" name="ctl00$ContentPlaceHolder1$AsnadTreeServiceUC1$hfDocumentXML" id="ContentPlaceHolder1_AsnadTreeServiceUC1_hfDocumentXML" value="&lt;مسألة>&lt;رقم_الفقرة ربط=&quot;104622&quot;/>سُورَةُ الرَّحْمَنِ &lt;نه/>&lt;قرآن>&lt;آية الآية=&quot;9&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4910&quot;>وَأَقِيمُوا الْوَزْنَ &lt;/آية>&lt;/قرآن>يُرِيدُ لِسَانَ الْمِيزَانِ وَالْعَصْفُ بَقْلُ الزَّرْعِ إِذَا قُطِعَ مِنْهُ شَيْءٌ قَبْلَ أَنْ يُدْرِكَ فَذَلِكَ الْعَصْفُ &lt;قرآن>&lt;آية الآية=&quot;12&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4913&quot;>وَالرَّيْحَانُ &lt;/آية>&lt;/قرآن>رِزْقُهُ &lt;قرآن>&lt;آية الآية=&quot;12&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4913&quot;>وَالْحَبُّ &lt;/آية>&lt;/قرآن>الَّذِي يُؤْكَلُ مِنْهُ وَالرَّيْحَانُ فِي كَلَامِ الْعَرَبِ الرِّزْقُ وَقَالَ بَعْضُهُمْ : وَالْعَصْفُ يُرِيدُ الْمَأْكُولَ مِنَ الْحَبِّ وَالرَّيْحَانُ النَّضِيجُ الَّذِي لَمْ يُؤْكَلْ . &lt;نه/>وَقَالَ غَيْرُهُ : الْعَصْفُ وَرَقُ الْحِنْطَةِ . وَقَالَ &lt;علم_رجل ربط=&quot;8115&quot;>الضَّحَّاكُ &lt;/علم_رجل>: &lt;متن_معلق>الْعَصْفُ التِّبْنُ &lt;/متن_معلق>. وَقَالَ &lt;علم_رجل ربط=&quot;2542&quot;>أَبُو مَالِكٍ &lt;/علم_رجل>: &lt;متن_معلق>الْعَصْفُ أَوَّلُ مَا يَنْبُتُ تُسَمِّيهِ &lt;علم_جماعة>النَّبَطُ &lt;/علم_جماعة>&lt;غريب ربط=&quot;15836&quot;>هَبُورًا &lt;/غريب>&lt;/متن_معلق>. وَقَالَ &lt;علم_رجل ربط=&quot;21453&quot;>مُجَاهِدٌ &lt;/علم_رجل>: &lt;متن_معلق>الْعَصْفُ وَرَقُ الْحِنْطَةِ &lt;/متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;12&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4913&quot;>وَالرَّيْحَانُ &lt;/آية>&lt;/قرآن>الرِّزْقُ ، &lt;غريب ربط=&quot;13584&quot;>وَالْمَارِجُ &lt;/غريب>اللهَبُ الْأَصْفَرُ وَالْأَخْضَرُ الَّذِي يَعْلُو النَّارَ إِذَا أُوقِدَتْ . &lt;نه/>وَقَالَ بَعْضُهُمْ عَنْ &lt;علم_رجل ربط=&quot;21453&quot;>مُجَاهِدٍ &lt;/علم_رجل>: &lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;17&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4918&quot;>رَبُّ الْمَشْرِقَيْنِ &lt;/آية>&lt;/قرآن>لِلشَّمْسِ ، فِي الشِّتَاءِ مَشْرِقٌ ، وَمَشْرِقٌ فِي الصَّيْفِ &lt;/متن_معلق>، &lt;قرآن>&lt;آية الآية=&quot;17&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4918&quot;>وَرَبُّ الْمَغْرِبَيْنِ &lt;/آية>&lt;/قرآن>مَغْرِبُهَا فِي الشِّتَاءِ وَالصَّيْفِ ، &lt;قرآن>&lt;آية الآية=&quot;20&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4921&quot;>لا يَبْغِيَانِ &lt;/آية>&lt;/قرآن>لَا يَخْتَلِطَانِ ، &lt;قرآن>&lt;آية الآية=&quot;24&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4925&quot;>الْمُنْشَآتُ &lt;/آية>&lt;/قرآن>مَا رُفِعَ &lt;غريب ربط=&quot;12155&quot;>قِلْعُهُ &lt;/غريب>مِنَ السُّفُنِ ، فَأَمَّا مَا لَمْ يُرْفَعْ &lt;غريب ربط=&quot;12156&quot;>قَلْعُهُ &lt;/غريب>فَلَيْسَ بِمُنْشَأَةٍ . وَقَالَ &lt;علم_رجل ربط=&quot;21453&quot;>مُجَاهِدٌ &lt;/علم_رجل>: &lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;35&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4936&quot;>وَنُحَاسٌ &lt;/آية>&lt;/قرآن>الصُّفْرُ يُصَبُّ عَلَى رُءُوسِهِمْ يُعَذَّبُونَ بِهِ &lt;/متن_معلق>، &lt;قرآن>&lt;آية الآية=&quot;46&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4947&quot;>خَافَ مَقَامَ رَبِّهِ &lt;/آية>&lt;/قرآن>يَهُمُّ بِالْمَعْصِيَةِ فَيَذْكُرُ اللهَ عَزَّ وَجَلَّ فَيَتْرُكُهَا ، الشُّوَاظُ لَهَبٌ مِنْ نَارٍ ، &lt;قرآن>&lt;آية الآية=&quot;64&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4965&quot;>مُدْهَامَّتَانِ &lt;/آية>&lt;/قرآن>سَوْدَاوَانِ مِنَ الرِّيِّ ، &lt;قرآن>&lt;آية الآية=&quot;14&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4915&quot;>صَلْصَالٍ &lt;/آية>&lt;/قرآن>طِينٌ خُلِطَ بِرَمْلٍ فَصَلْصَلَ كَمَا يُصَلْصِلُ الْفَخَّارُ . وَيُقَالُ : مُنْتِنٌ يُرِيدُونَ بِهِ صَلَّ ، يُقَالُ : صَلْصَالٌ كَمَا يُقَالُ صَرَّ الْبَابُ عِنْدَ الْإِغْلَاقِ ، وَصَرْصَرَ مِثْلُ كَبْكَبْتُهُ ، يَعْنِي كَبَبْتُهُ ، &lt;قرآن>&lt;آية الآية=&quot;68&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4969&quot;>فَاكِهَةٌ وَنَخْلٌ وَرُمَّانٌ &lt;/آية>&lt;/قرآن>. وَقَالَ بَعْضُهُمْ : لَيْسَ الرُّمَّانُ وَالنَّخْلُ بِالْفَاكِهَةِ ، وَأَمَّا الْعَرَبُ فَإِنَّهَا تَعُدُّهَا فَاكِهَةً كَقَوْلِهِ عَزَّ وَجَلَّ : &lt;قرآن>&lt;آية الآية=&quot;238&quot; السورة=&quot;البقرة&quot; ربط=&quot;245&quot;>حَافِظُوا عَلَى الصَّلَوَاتِ وَالصَّلاةِ الْوُسْطَى &lt;/آية>&lt;/قرآن>&lt;الصفحات جزء=&quot;6&quot; صفحة=&quot;145&quot;/>فَأَمَرَهُمْ بِالْمُحَافَظَةِ عَلَى كُلِّ الصَّلَوَاتِ ثُمَّ أَعَادَ الْعَصْرَ تَشْدِيدًا لَهَا كَمَا أُعِيدَ النَّخْلُ وَالرُّمَّانُ ، وَمِثْلُهَا &lt;قرآن>&lt;آية الآية=&quot;18&quot; السورة=&quot;الحج&quot; ربط=&quot;2613&quot;>أَلَمْ تَرَ أَنَّ اللهَ يَسْجُدُ لَهُ مَنْ فِي السَّمَاوَاتِ وَمَنْ فِي الأَرْضِ &lt;/آية>&lt;/قرآن>، ثُمَّ قَالَ : &lt;قرآن>&lt;آية الآية=&quot;18&quot; السورة=&quot;الحج&quot; ربط=&quot;2613&quot;>وَكَثِيرٌ مِنَ النَّاسِ وَكَثِيرٌ حَقَّ عَلَيْهِ الْعَذَابُ &lt;/آية>&lt;/قرآن>، وَقَدْ ذَكَرَهُمْ فِي أَوَّلِ قَوْلِهِ : &lt;قرآن>&lt;آية الآية=&quot;18&quot; السورة=&quot;الحج&quot; ربط=&quot;2613&quot;>مَنْ فِي السَّمَاوَاتِ وَمَنْ فِي الأَرْضِ &lt;/آية>&lt;/قرآن>، وَقَالَ غَيْرُهُ : &lt;قرآن>&lt;آية الآية=&quot;48&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4949&quot;>أَفْنَانٍ &lt;/آية>&lt;/قرآن>أَغْصَانٍ ، &lt;قرآن>&lt;آية الآية=&quot;54&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4955&quot;>وَجَنَى الْجَنَّتَيْنِ دَانٍ &lt;/آية>&lt;/قرآن>مَا يُجْتَنَى قَرِيبٌ . وَقَالَ &lt;علم_رجل ربط=&quot;7222&quot;>الْحَسَنُ : &lt;/علم_رجل>&lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;13&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4914&quot;>فَبِأَيِّ آلاءِ &lt;/آية>&lt;/قرآن>نِعَمِهِ &lt;/متن_معلق>. وَقَالَ &lt;علم_رجل ربط=&quot;20599&quot;>قَتَادَةُ &lt;/علم_رجل>: &lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;13&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4914&quot;>رَبِّكُمَا &lt;/آية>&lt;/قرآن>يَعْنِي الْجِنَّ وَالْإِنْسَ . &lt;/متن_معلق>وَقَالَ &lt;علم_رجل ربط=&quot;559&quot;>أَبُو الدَّرْدَاءِ : &lt;/علم_رجل>&lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;29&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4930&quot;>كُلَّ يَوْمٍ هُوَ فِي شَأْنٍ &lt;/آية>&lt;/قرآن>يَغْفِرُ ذَنْبًا وَيَكْشِفُ كَرْبًا وَيَرْفَعُ قَوْمًا وَيَضَعُ آخَرِينَ &lt;/متن_معلق>. &lt;نه/>وَقَالَ &lt;علم_رجل ربط=&quot;6051&quot;>ابْنُ عَبَّاسٍ &lt;/علم_رجل>: &lt;متن_معلق>&lt;قرآن>&lt;آية الآية=&quot;20&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4921&quot;>بَرْزَخٌ &lt;/آية>&lt;/قرآن>حَاجِزٌ &lt;/متن_معلق>، الْأَنَامُ الْخَلْقُ &lt;قرآن>&lt;آية الآية=&quot;66&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4967&quot;>نَضَّاخَتَانِ &lt;/آية>&lt;/قرآن>فَيَّاضَتَانِ ، &lt;قرآن>&lt;آية الآية=&quot;27&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4928&quot;>ذُو الْجَلالِ &lt;/آية>&lt;/قرآن>ذُو الْعَظَمَةِ . وَقَالَ غَيْرُهُ : &lt;غريب ربط=&quot;13584&quot;>مَارِجٌ &lt;/غريب>خَالِصٌ مِنَ النَّارِ ، يُقَالُ : مَرَجَ الْأَمِيرُ رَعِيَّتَهُ إِذَا خَلَّاهُمْ يَعْدُو بَعْضُهُمْ عَلَى بَعْضٍ ، مَرَجَ أَمْرُ النَّاسِ ، &lt;قرآن>&lt;آية الآية=&quot;5&quot; السورة=&quot;ق&quot; ربط=&quot;4635&quot;>مَرِيجٍ &lt;/آية>&lt;/قرآن>مُلْتَبِسٌ ، &lt;قرآن>&lt;آية الآية=&quot;19&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4920&quot;>مَرَجَ &lt;/آية>&lt;/قرآن>اخْتَلَطَ الْبَحْرَانِ ، مِنْ مَرَجْتَ دَابَّتَكَ تَرَكْتَهَا ، &lt;قرآن>&lt;آية الآية=&quot;31&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4932&quot;>سَنَفْرُغُ لَكُمْ &lt;/آية>&lt;/قرآن>سَنُحَاسِبُكُمْ لَا يَشْغَلُهُ شَيْءٌ عَنْ شَيْءٍ ، وَهُوَ مَعْرُوفٌ فِي كَلَامِ الْعَرَبِ يُقَالُ : لَأَتَفَرَّغَنَّ لَكَ ، وَمَا بِهِ شُغْلٌ ، يَقُولُ : لَآخُذَنَّكَ عَلَى غِرَّتِكَ . &lt;نه/>&lt;قرآن>&lt;آية الآية=&quot;62&quot; السورة=&quot;الرحمن&quot; ربط=&quot;4963&quot;>وَمِنْ دُونِهِمَا جَنَّتَانِ &lt;/آية>&lt;/قرآن>&lt;نه/>&lt;رقم_حديث نوع=&quot;حرف&quot;>4680 &lt;/رقم_حديث>&lt;رقم_حديث نوع=&quot;مطبوع&quot;>4878 &lt;/رقم_حديث>- &lt;حديث تخصيص=&quot;قولية&quot; نوع=&quot;مرفوع&quot;>&lt;مصطلح_صيغ ربط=&quot; 110 &quot;>&lt;صيغة_تحديث>حَدَّثَنَا &lt;/صيغة_تحديث>&lt;/مصطلح_صيغ>&lt;راوي اسم=&quot;عبد الله بن أبي الأسود&quot; ربط=&quot;3885&quot; نوع=&quot;شيخ_المصنف&quot;>عَبْدُ اللهِ بْنُ أَبِي الْأَسْوَدِ ، &lt;/راوي>&lt;مصطلح_صيغ ربط=&quot; 110 &quot;>&lt;صيغة_تحديث>حَدَّثَنَا &lt;/صيغة_تحديث>&lt;/مصطلح_صيغ>&lt;راوي اسم=&quot;عبد العزيز بن عبد الصمد العمي&quot; ربط=&quot;3480&quot; نوع=&quot;رواة_الحديث&quot;>عَبْدُ الْعَزِيزِ بْنُ عَبْدِ الصَّمَدِ الْعَمِّيُّ &lt;/راوي>، &lt;مصطلح_صيغ ربط=&quot; 110 &quot;>&lt;صيغة_تحديث>حَدَّثَنَا &lt;/صيغة_تحديث>&lt;/مصطلح_صيغ>&lt;راوي اسم=&quot;أبو عمران الجوني&quot; ربط=&quot;4019&quot; نوع=&quot;رواة_الحديث&quot;>أَبُو عِمْرَانَ الْجَوْنِيُّ ، &lt;/راوي>&lt;مصطلح_صيغ ربط=&quot; 110 &quot;>&lt;صيغة_تحديث>عَنْ &lt;/صيغة_تحديث>&lt;/مصطلح_صيغ>&lt;سند نوع=&quot;لطائف&quot;>&lt;راوي اسم=&quot;أبي بكر بن عبد الله بن قيس&quot; ربط=&quot;219&quot; نوع=&quot;رواة_الحديث&quot;>أَبِي بَكْرِ بْنِ عَبْدِ اللهِ بْنِ قَيْسٍ ، &lt;/راوي>&lt;مصطلح_صيغ ربط=&quot; 110 &quot;>&lt;صيغة_تحديث>عَنْ &lt;/صيغة_تحديث>&lt;/مصطلح_صيغ>&lt;راوي اسم=&quot;أبيه&quot; ربط=&quot;3856&quot; نوع=&quot;راوي_الحديث&quot;>أَبِيهِ &lt;/راوي>&lt;/سند>: أَنَّ رَسُولَ اللهِ صَلَّى اللهُ عَلَيْهِ وَسَلَّمَ قَالَ : &lt;مصطلح_متون ربط=&quot; 51 54 &quot;>&lt;متن ربط=&quot;1046221&quot; نوع=&quot;مرفوع&quot;>&lt;طرف>جَنَّتَانِ مِنْ فِضَّةٍ ، آنِيَتُهُمَا وَمَا فِيهِمَا &lt;/طرف>، وَجَنَّتَانِ مِنْ ذَهَبٍ ، آنِيَتُهُمَا وَمَا فِيهِمَا ، وَمَا بَيْنَ الْقَوْمِ وَبَيْنَ أَنْ يَنْظُرُوا إِلَى رَبِّهِمْ إِلَّا رِدَاءُ الْكِبْرِ عَلَى وَجْهِهِ فِي جَنَّةِ عَدْنٍ &lt;/متن>&lt;/مصطلح_متون>. &lt;نه/>&lt;/حديث>&lt;/مسألة>" />

