<script type="text/javascript">
    function ViewHidePartPageNumber() {
        if (document.getElementsByName('PartPageSpan')) {
            PartPageTags = document.getElementsByName('PartPageSpan');
            PrevDisplayType = PartPageTags[0].style.display;
            CurrDisplayType = PrevDisplayType == 'none' ? '' : 'none';
            document.getElementById('ContentPlaceHolder1_ucViewHadith_hidHidePartPage').value = CurrDisplayType;
            for (iPartPage = 0; iPartPage < PartPageTags.length; iPartPage++) {
                PartPageTags[iPartPage].style.display = CurrDisplayType;
            }
        }
        ///////////
        var xmlString = document.getElementById('ContentPlaceHolder1_ucViewHadith_hidContent').value;
        var parser = new DOMParser();
        var doc = parser.parseFromString(xmlString, "text/html");

        if (doc.getElementsByName('PartPageSpan')) {
            for (i = 0; i < doc.getElementsByName('PartPageSpan').length; i++) {
                doc.getElementsByName('PartPageSpan')[i].style.display = CurrDisplayType;
            }
        }
        document.getElementById('ContentPlaceHolder1_ucViewHadith_hidContent').value = doc.firstChild.outerHTML;
    }


    function OnTarqeemTypeChange() {
        var dropdown = document.getElementById('ContentPlaceHolder1_ucViewHadith_ddlTarqeemType');
        var myindex = dropdown.selectedIndex;
        var otherindex = myindex == 0 ? 1 : 0;
        var SelValue = dropdown.options[myindex].value;
        document.getElementById('ContentPlaceHolder1_ucViewHadith_hidTarqeemType').value = SelValue;
        if (document.getElementById(SelValue))
            document.getElementById('ContentPlaceHolder1_ucViewHadith_txtHadithNumber').value = document.getElementById(SelValue).innerText.trim();
        var OtherValue = dropdown.options[otherindex].value;

        if (document.getElementsByName(SelValue)) {
            for (iSel = 0; iSel < document.getElementsByName(SelValue).length; iSel++) {
                document.getElementsByName(SelValue)[iSel].style.display = '';
            }
        }
        
        if (document.getElementsByName(OtherValue)) {
            for (iOther = 0; iOther < document.getElementsByName(OtherValue).length; iOther++) {
                document.getElementsByName(OtherValue)[iOther].style.display = 'none';
            }
        }
        ///////////
        var xmlString = document.getElementById('ContentPlaceHolder1_ucViewHadith_hidContent').value;
        var parser = new DOMParser();
        var doc = parser.parseFromString(xmlString, "text/html");

        if (doc.getElementsByName(SelValue)) {
            for (i = 0; i < doc.getElementsByName(SelValue).length; i++) {
                doc.getElementsByName(SelValue)[i].style.display = '';
            }
        }

        if (doc.getElementsByName(OtherValue)) {
            for (i = 0; i < doc.getElementsByName(OtherValue).length; i++) {
                doc.getElementsByName(OtherValue)[i].style.display = 'none';
            }
        }
        document.getElementById('ContentPlaceHolder1_ucViewHadith_hidContent').value = doc.firstChild.outerHTML;
    }
</script>
