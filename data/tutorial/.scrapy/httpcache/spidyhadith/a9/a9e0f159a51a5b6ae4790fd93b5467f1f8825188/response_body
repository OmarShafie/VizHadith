<input name="ctl00$ContentPlaceHolder1$ucViewHadith$hidContent" type="hidden" id="ContentPlaceHolder1_ucViewHadith_hidContent" value="&lt;html dir=&quot;rtl&quot; xmlns:msxsl=&quot;urn:schemas-microsoft-com:xslt&quot; xmlns:user=&quot;http://mycompany.com/mynamespace&quot; xmlns:html=&quot;http://www.w3.org/Profiles/XHTML-transitional&quot; xmlns:asp=&quot;http://example.com/asp&quot;>
