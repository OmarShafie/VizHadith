<script type="text/javascript">
    function GetFormulas() {
        var RwayID = document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_hidRawyId").value;
        var ReturnedValues = window.showModalDialog('Formulas.aspx?RawyID=' + RwayID + '', null, 'status:no;dialogWidth:300px;dialogHeight:300px;dialogHide:false;help:no;scroll:yes;resizable:no');
        var vRetValAttr = new Array();
        vRetValAttr = ReturnedValues.split(';');
        var varlbFormulas = document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_lbFormulas");
        varlbFormulas.options.length = 0;
        var hidTahdithIds = '';
        for (var i = 0; i < vRetValAttr.length; i++) {
            var optn = document.createElement("option");
            optn.text = vRetValAttr[i].split('_')[0];
            optn.value = vRetValAttr[i].split('_')[1];
            varlbFormulas.options.add(optn);
            hidTahdithIds += optn.value;
            hidTahdithIds += ',';
        }
        hidTahdithIds = hidTahdithIds.substring(0, hidTahdithIds.length - 1);
        document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_hidTahdithIds").value = hidTahdithIds;
    }

    function openWin() {
        var RwayID = document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_hidRawyId").value;
        //var oWnd = radopen("Formulas.aspx?RawyID=" + RwayID, "RadWindow1");
        var oWnd = $find("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_RadWindow1");
        oWnd.setUrl("Formulas.aspx?RawyID=" + RwayID);
        oWnd.show();
    }

    function OnClientClose(oWnd, args) {
        //get the transferred arguments
        var arg = args.get_argument();
        if (arg) {
            var ReturnedValues = arg.returnValue;
            var vRetValAttr = new Array();
            vRetValAttr = ReturnedValues.split(';');
            var varlbFormulas = document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_lbFormulas");
            varlbFormulas.options.length = 0;
            var hidTahdithIds = '';
            for (var i = 0; i < vRetValAttr.length; i++) {
                var optn = document.createElement("option");
                optn.text = vRetValAttr[i].split('_')[0];
                optn.value = vRetValAttr[i].split('_')[1];
                varlbFormulas.options.add(optn);
                hidTahdithIds += optn.value;
                hidTahdithIds += ',';
            }
            hidTahdithIds = hidTahdithIds.substring(0, hidTahdithIds.length - 1);
            document.getElementById("ContentPlaceHolder1_ViewRwahUC1_TahdithFormulasUC1_hidTahdithIds").value = hidTahdithIds;
    }
}
</script>

