<script type="text/javascript">
    function selectBook(selectedCheckBox) {
        var GridVwHeaderChckbox = selectedCheckBox.parentNode.parentNode.parentNode;
        var AllCheckedStatus = selectedCheckBox.checked;
        for (i = 1; i < GridVwHeaderChckbox.rows.length; i++) {
            GridVwHeaderChckbox.rows[i].cells[0].getElementsByTagName("INPUT")[0].checked = AllCheckedStatus;
        } // end for
    }

    function selectsingleBook(selectedCheckBox) {
        var GridVwHeaderChckbox = selectedCheckBox.parentNode.parentNode.parentNode;
        var AllSelectedCount = 0;
        for (i = 1; i < GridVwHeaderChckbox.rows.length; i++) {
            if (GridVwHeaderChckbox.rows[i].cells[0].getElementsByTagName("INPUT")[0].checked) {
                AllSelectedCount += 1;
            }
        } // end for
        if (AllSelectedCount == GridVwHeaderChckbox.rows.length - 1)
            GridVwHeaderChckbox.rows[0].cells[0].getElementsByTagName("INPUT")[0].checked = true;
        else
            GridVwHeaderChckbox.rows[0].cells[0].getElementsByTagName("INPUT")[0].checked = false;
    }

</script>


