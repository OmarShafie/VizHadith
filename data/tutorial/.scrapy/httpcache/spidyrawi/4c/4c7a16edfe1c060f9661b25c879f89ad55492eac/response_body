<script language="javascript" type="text/javascript">
    function checkFieldSelection() {
        var varAnyOneSelected = false;
        if (document.getElementById('ContentPlaceHolder1_ViewRwahUC1_cbAllSelected').checked)
            varAnyOneSelected = true;
        else {
            var checkBoxArray = document.getElementById('ContentPlaceHolder1_ViewRwahUC1_cblSearchField').getElementsByTagName('input');
            for (var i = 0; i < checkBoxArray.length; i++) {
                if (checkBoxArray[i].checked) {
                    varAnyOneSelected = true;
                    break;
                }
            }
        } // end else
        if (!varAnyOneSelected) {
            alert('برجاء اختيار أحد عناصر البحث');
            return false;
        }
    }

    function cbSelectionChanged(SelectedElem) {
        var varAllSelected = true;
        if (!SelectedElem.checked)
            varAllSelected = false;

        var checkBoxArray = document.getElementById('ContentPlaceHolder1_ViewRwahUC1_cblSearchField').getElementsByTagName('input');
        for (var i = 0; i < checkBoxArray.length; i++) {
            checkBoxArray[i].checked = varAllSelected;
        }
    }// end function


    function cblSelectionChanged(SelectedElem) {
        var checkBoxArray = SelectedElem.getElementsByTagName('input');
        var varAllSelected = true;
        for (var i = 0; i < checkBoxArray.length; i++) {
            if (!checkBoxArray[i].checked) {
                varAllSelected = false;
                break;
            } // end if all   
        } // end for
        document.getElementById('ContentPlaceHolder1_ViewRwahUC1_cbAllSelected').checked = varAllSelected;
    }
</script>
